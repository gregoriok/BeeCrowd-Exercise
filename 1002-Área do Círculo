import java.util.Scanner;
import java.lang.Math;
public class App {
    
    public static void main(String[] args) throws Exception {
        Scanner ler = new Scanner(System.in);
        Double raio,A;
        raio = ler.nextDouble();
        A = 3.14159 *Math.pow(raio, 2);
       System.out.printf("A=%.4f\n",A);
    }
}
