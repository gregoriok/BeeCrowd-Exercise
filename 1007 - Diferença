import java.util.Scanner;

public class App {
    
    public static void main(String[] args) throws Exception {
        Scanner ler = new Scanner(System.in);
        int A,B,C,D,DIFERENCA;
        A = ler.nextInt();
        B = ler.nextInt();
        C = ler.nextInt();
        D = ler.nextInt();
        DIFERENCA = (A * B - C * D);
       System.out.printf("DIFERENCA = %d\n",DIFERENCA);
    }
}
