import java.util.Scanner;

public class App {
    
    public static void main(String[] args) throws Exception {
        Scanner ler = new Scanner(System.in);
        Double A,B,C,MEDIA;
        A = ler.nextDouble();
        B = ler.nextDouble();
        C = ler.nextDouble();
        MEDIA = ((A*2)+(B*3)+(C*5))/10;
       System.out.printf("MEDIA = %.1f\n",MEDIA);
    }
}
